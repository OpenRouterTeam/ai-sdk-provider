{
  "name": "@openrouter/ai-provider",
  "version": "2.0.0-alpha.0",
  "description": "OpenRouter provider for the Vercel AI SDK",
  "keywords": [
    "ai",
    "openrouter",
    "vercel-ai",
    "ai-sdk",
    "llm",
    "language-model",
    "embeddings",
    "image-generation"
  ],
  "homepage": "https://github.com/openrouter/ai-provider",
  "repository": {
    "type": "git",
    "url": "https://github.com/openrouter/ai-provider.git"
  },
  "author": "OpenRouter",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "stylecheck": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.17",
    "@openrouter/sdk": "^0.1.27",
    "ai": "^5.0.98",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@changesets/cli": "2.29.8",
    "@types/node": "20.19.0",
    "dotenv": "17.2.3",
    "tsup": "8.1.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.13"
  },
  "peerDependencies": {
    "zod": "^4.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
