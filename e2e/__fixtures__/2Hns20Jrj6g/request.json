{
  "url": "https://openrouter.ai/api/v1/chat/completions",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json",
    "Authorization": "[REDACTED]"
  },
  "body": {
    "json": {
      "model": "anthropic/claude-sonnet-4",
      "messages": [
        {
          "role": "user",
          "content": [
            {
              "type": "input_text",
              "text": ""
            },
            {
              "type": "input_text",
              "text": "How many \"a\" did I use in the previous message?"
            }
          ]
        }
      ],
      "stream": true,
      "stream_options": {
        "include_usage": true
      },
      "provider": {
        "order": [
          "Anthropic"
        ]
      }
    }
  }
}