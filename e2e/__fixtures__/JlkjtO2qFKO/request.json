{
  "url": "https://openrouter.ai/api/v1/chat/completions",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json",
    "Authorization": "[REDACTED]"
  },
  "body": {
    "json": {
      "model": "anthropic/claude-3.7-sonnet:thinking",
      "messages": [
        {
          "role": "user",
          "content": [
            {
              "type": "text",
              "text": "What is the capital of France?"
            }
          ]
        }
      ],
      "stream": true,
      "stream_options": {
        "include_usage": true
      }
    }
  }
}