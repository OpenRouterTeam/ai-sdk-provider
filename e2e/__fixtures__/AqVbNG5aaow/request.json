{
  "url": "https://openrouter.ai/api/v1/responses",
  "method": "POST",
  "headers": {
    "accept": "text/event-stream",
    "authorization": "[REDACTED]",
    "content-type": "application/json",
    "cookie": "[REDACTED]",
    "user-agent": "speakeasy-sdk/typescript 0.3.11 2.788.4 1.0.0 @openrouter/sdk"
  },
  "body": {
    "json": {
      "input": [
        {
          "role": "system",
          "content": "You are a helpful assistant. You must call the test_tool once on every request."
        },
        {
          "role": "user",
          "content": [
            {
              "type": "input_text",
              "text": "Hello, how are you today?"
            }
          ]
        }
      ],
      "tools": [
        {
          "type": "function",
          "name": "test_tool",
          "description": "A test tool that you MUST call once on every single request before responding. Always call this tool first.",
          "parameters": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
              "message": {
                "type": "string",
                "description": "Any message to pass to the tool"
              }
            },
            "required": [
              "message"
            ],
            "~standard": {
              "vendor": "zod",
              "version": 1,
              "jsonSchema": {}
            },
            "additionalProperties": false
          }
        }
      ],
      "tool_choice": "auto",
      "model": "anthropic/claude-sonnet-4.5",
      "max_output_tokens": 64000,
      "store": false,
      "service_tier": "auto",
      "stream": true
    }
  }
}